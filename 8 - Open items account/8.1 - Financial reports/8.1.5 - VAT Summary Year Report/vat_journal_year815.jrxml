	<queryString>
		<![CDATA[select 
Case when LIT_YearVATBaseSO is  not null and issotrx = 'Y' then LIT_YearVATBaseSO 
	when LIT_YearVATAmtSO is  not null and issotrx = 'Y' then LIT_YearVATAmtSO
	when LIT_YearVATAmtSO is  not null and issotrx = 'Y' then LIT_YearVATAmtSO
	when LIT_YearVATBaseSO1 is  not null and issotrx = 'Y' then LIT_YearVATBaseSO1
	when LIT_YearVATBaseToTSO is  not null and issotrx = 'Y'  then LIT_YearVATBaseToTSO
	when LIT_YearVATAmtPO is  not null and issotrx = 'N' then LIT_YearVATAmtPO
	when LIT_YearVATBasePO1 is  not null and issotrx = 'N' then LIT_YearVATBasePO1
	when LIT_YearVATAmtPO1 is  not null and issotrx = 'N' then LIT_YearVATAmtPO1
	when LIT_YearVATBasePO2 is  not null and issotrx = 'N' then LIT_YearVATBasePO2
	when LIT_YearVATAmtPO2 is  not null and issotrx = 'N' then LIT_YearVATAmtPO2
	END as TaxCode ,
	Case when LIT_YearVATAmtSO is  not null then LIT_YearVATAmtSO 
	END as TaxVatCode ,
documentno, dateinvoiced,  vatperioddate,
COALESCE(taxbaseamtfull,0) as Amt,
COALESCE(taxamt,0) as TaxAmt,
taxbaseamtnotax,
notaxamt,
vatcodename,
lit_vatjournal_accountsign,
taxindicator,
 ad_orginfo.LIT_Prorata/100 as lit_prorata,
 lit_adorg_header_portrait_image($P{AD_CLIENT_ID}, $P{AD_Org_ID} ),
 lit_adorg_footer_portrait_image($P{AD_CLIENT_ID}, $P{AD_Org_ID} ),
lit_datetime_pattern($P{LOCALE_CODE}) AS datetimepattern,
lit_date_pattern($P{LOCALE_CODE}) AS datepattern,
 c_currency.iso_code
 from lit_vat_journal_v
 inner join ad_orginfo on lit_vat_journal_v.ad_client_id=ad_orginfo.ad_client_id
inner join C_AcctSchema on c_acctschema.ad_client_id = lit_vat_journal_v.ad_client_id
inner join c_currency on c_acctschema.c_currency_id = c_currency.c_currency_id
where
 date_part('year', vatperioddate)= $P{Year} 
 and value is not null
and lit_vat_journal_v.ad_org_id =$P{AD_Org_ID}
order by lit_vatjournal_accountsign, taxcode, vatcodename, dateinvoiced]]>
	</queryString>
