	<queryString>
		<![CDATA[select
	c_bpartner.name as c_bpartner_name,
	c_bpartner_location.phone,
	c_bpartner_location.phone2,
	c_bpartner_location.fax,
	c_location.address1,
	c_location.address2,
	c_location.city,
	c_location.postal,
	c_location.postal_add,
 	lit_adorg_header_portrait_image(c_bpartner.ad_client_id, c_bpartner.ad_org_id),
	lit_adorg_footer_portrait_image(c_bpartner.ad_client_id, c_bpartner.ad_org_id),
	lit_date_pattern($P{LOCALE_CODE}) AS datepattern,
	lit_time_pattern($P{LOCALE_CODE}) AS timepattern,
	(
	select
		coalesce(SUM(iar.GRANDTOTAL), 0)
	from
		C_INVOICE iar
	inner join C_DOCTYPE doc on
		iar.C_DOCTYPE_ID = doc.C_DOCTYPE_ID
	where
		doc.docbasetype in ('ARI', 'ARC')
		and iar.DOCSTATUS in ('CO', 'CL')
		and iar.C_BPARTNER_ID = $P{C_BPartner_ID}
		and iar.DATEINVOICED < $P{DateAcctFrom}
		)
		-(
	select
		coalesce(SUM(par.PAYAMT) + SUM(par.discountamt), 0) as ARPayments
	from
		C_PAYMENT par
	inner join C_DOCTYPE doc on
		par.C_DOCTYPE_ID = doc.C_DOCTYPE_ID
	where
		doc.docbasetype in ('ARR')
		and par.DOCSTATUS in ('CO', 'CL')
			and par.C_BPARTNER_ID = $P{C_BPartner_ID}
			and par.DATETRX < $P{DateAcctFrom}
	      ) as OpenningBal,
	X.*
from
	c_bpartner
inner join (
	select
	inv.c_bpartner_id,
	inv.DATEINVOICED as DateTrx,
	inv.DOCUMENTNO,
	coalesce(doctrl.name, doc.Name) as name,
	inv.DESCRIPTION,
	c_currency.iso_code,
	case
		when doc.docbasetype in ('ARC') then inv.GRANDTOTAL * -1
		else inv.GRANDTOTAL
	end as "GRANDTOTAL"
from
	C_INVOICE inv
join
    C_DOCTYPE doc on
	inv.C_DOCTYPE_ID = doc.C_DOCTYPE_ID
inner join c_currency on inv.c_currency_id = c_currency.c_currency_id
left join c_doctype_trl doctrl on doc.c_doctype_id = doctrl.c_doctype_id and doctrl.ad_language = $P{LOCALE_CODE}
where
	inv.DOCSTATUS in ('CO', 'CL')
	and doc.docbasetype in ('ARI', 'ARC')
	and inv.C_BPARTNER_ID = $P{C_BPartner_ID}
	and inv.DATEINVOICED between $P{DateAcctFrom} and $P{DateAcctTo}	
union all
select
	pa.c_bpartner_id,
	pa.DATETRX as DateTrx,
	pa.DOCUMENTNO,
	coalesce(doctrl.name, doc.Name) as name,
	'' as DESCRIPTION,
	c_currency.iso_code,
	pa.discountamt * -1 as "GRANDTOTAL"
from
	C_PAYMENT pa
join
    C_DOCTYPE doc on
	pa.C_DOCTYPE_ID = doc.C_DOCTYPE_ID
inner join c_currency on pa.c_currency_id = c_currency.c_currency_id
left join c_doctype_trl doctrl on doc.c_doctype_id = doctrl.c_doctype_id and doctrl.ad_language = $P{LOCALE_CODE}
where
	pa.DOCSTATUS in ('CO', 'CL')
	and doc.docbasetype in ('ARR')
	and pa.C_BPARTNER_ID = $P{C_BPartner_ID}
	and pa.DATETRX between $P{DateAcctFrom} and $P{DateAcctTo}
	and pa.discountamt > 0
union all
select
	pa.c_bpartner_id,
	pa.DATETRX as DateTrx,
	pa.DOCUMENTNO,
	coalesce(doctrl.name, doc.Name) as name,
	coalesce(pa.DESCRIPTION, inv.DOCUMENTNO) as DESCRIPTION,
	c_currency.iso_code,
	(pa.PAYAMT * -1) - pa.writeoffamt as "GRANDTOTAL"
from
	C_PAYMENT pa
join
    C_DOCTYPE doc on
	pa.C_DOCTYPE_ID = doc.C_DOCTYPE_ID
inner join c_currency on pa.c_currency_id = c_currency.c_currency_id
left join c_doctype_trl doctrl on doc.c_doctype_id = doctrl.c_doctype_id and doctrl.ad_language = $P{LOCALE_CODE}
left join
    C_INVOICE inv on
	pa.C_INVOICE_ID = inv.C_INVOICE_ID
where
	pa.DOCSTATUS in ('CO', 'CL')
	and doc.docbasetype in ('ARR')
	and pa.C_BPARTNER_ID = $P{C_BPartner_ID}
	and pa.DATETRX between $P{DateAcctFrom} and $P{DateAcctTo}
order by
	DateTrx
) X on c_bpartner.c_bpartner_id = X.c_bpartner_id
left join c_bpartner_location on
	c_bpartner.c_bpartner_id = c_bpartner_location.c_bpartner_id and c_bpartner_location.isbillto = 'Y'
left join c_location on
	c_bpartner_location.c_location_id = c_location.c_location_id
where
	c_bpartner.c_bpartner_id = $P{C_BPartner_ID}
	
]]>
	</queryString>
